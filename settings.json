    "explorer.autoReveal": true,
    "explorer.confirmDelete": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.vue": "${capture}.ts, ${capture}.spec.vue, ${capture}.test.vue, ${capture}.spec.ts, ${capture}.test.ts, ${capture}.test.browser.ts, ${capture}.spec.js, ${capture}.test.js, ${capture}.test.browser.js, ${capture}.nuxt.spec.ts, ${capture}.nuxt.test.ts, ${capture}.story.vue, ${capture}.story.ts, ${capture}.story.js, ${capture}.stories.vue, ${capture}.stories.ts, ${capture}.stories.js",
        "*.ts": "${capture}.js, ${capture}.spec.ts, ${capture}.test.ts, ${capture}.nuxt.spec.ts, ${capture}.nuxt.test.ts",
        "*.js": "${capture}.spec.js, ${capture}.test.js, ${capture}.nuxt.spec.js, ${capture}.nuxt.test.js"
    },

    "git.confirmSync": false,
    "git.inputValidationSubjectLength": null,
    "git.replaceTagsWhenPull": true,

    "html.autoClosingTags": true,
    "css.format.spaceAroundSelectorSeparator": true,
    "css.lint.unknownAtRules": "ignore",
    "less.lint.unknownAtRules": "ignore",
    "scss.lint.unknownAtRules": "ignore",
    "vue.autoInsert.dotValue": true,
    "vue.suggest.componentNameCasing": "preferKebabCase",

    "prettier.printWidth": 100,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "none",

    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,

    "playwright.reuseBrowser": true,
    "playwright.showTrace": false,
    "playwright.env": {
        "VITE_TEST_MODE": true
    },

    "terminal.integrated.defaultProfile.linux": "local zsh",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "fish": {
            "path": "fish"
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "local zsh": {
            "path": "/usr/bin/zsh"
        }
    },

    "settingsSync.ignoredExtensions": [],

    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",

    "material-icon-theme.activeIconPack": "vue",

    "workbench.colorTheme": "Ayu Dark",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.hover.delay": 1000,
    "workbench.panel.defaultLocation": "right",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,

